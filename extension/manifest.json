{
  "manifest_version": 3,
  "name": "HIU Questionnaire",
  "version": "1.0",
  "permissions": ["tabs", "activeTab", "scripting", "storage"],
  "host_permissions": [
    "https://docs.google.com/*",
    "https://script.google.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
